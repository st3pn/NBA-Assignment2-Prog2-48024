<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.Pos?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="controller.SeasonController" alignment="CENTER" styleClass="background" prefHeight="450.0" prefWidth="100.0" spacing="20" xmlns:fx="http://javafx.com/fxml/1"
       >
    <ImageView>
        <image>
            <Image url="@NBA_BACK.jpg"/>
        </image>
    </ImageView>

    <VBox spacing="30" alignment="CENTER">
        <Button text="Round" onAction="#viewSeasonRound"/>
        <Button text="Current" onAction="#viewCurrentRoundTeams"/>
        <Button text="Game" onAction="#handleGame" />
        <Button text="Result"/>
        <Button text="Close" onAction="#close"/>
    </VBox>

    
        
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
    
</VBox>
